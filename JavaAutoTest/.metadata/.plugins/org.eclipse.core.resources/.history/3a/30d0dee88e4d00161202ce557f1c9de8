package helloword;
public class Susan_C{
	   public Susan_C(String name){
	      //这个构造器仅有一个参数：name
	      System.out.println("Passed Name is :" + name ); 
	   }
	   public static void main(String []args){
	      // 下面的语句将创建一个Susan_C对象
	      Susan_C myObject = new Susan_C( "tommy" );
	      myObject.notify();
	      
	   }
	}